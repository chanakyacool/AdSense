<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, autofocus: true %></div>

       <div><%= f.label :username %><br />
  <%= f.text_field :username, autocomplete: "off" %></div>

  <div><%= f.label :password %> <% if @validatable %><i>(<%= @minimum_password_length %> characters minimum)</i><% end %><br />
    <%= f.password_field :password, autocomplete: "off" %></div>

  <div><%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %></div>

     <div><%= f.label :date_of_birth %><br />
  <%= f.date_select :dob, start_year: 1940, end_year: Date.today.year , autocomplete: "off" %></div>


     <div><%= f.label :gender %><br />
  <%=  f.select :gender, options_for_select(%w[Male Female]) %></div>

     <div><%= f.label :economy_range %><br /> 
         <%= f.select :economy,  options_for_select(%w[0-10000 10000-50000 50000-100000 >100000 ]) %>


     <div><%= f.label :designation %><br />
  <%= f.select :designation,  options_for_select(%w[Student Employee BusinessOwner]) %></div>

     <div><%= f.label :country %><br />
   <%= f.select :country, :id => "country", :name =>"country" %></div>

     <div><%= f.label :state %><br />
   <%= f.select :state, :id =>"state"%> </div>
 

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>
 
 
        <script language="javascript">
            populateCountries("user_country", "user_state");  
        </script>
      